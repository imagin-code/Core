<?xml version="1.0" encoding="utf-8"?>
<Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="Imagin.Common.Controls">
  <Top/>
  <Left/>
  <Right/>
  <Bottom/>
  <Root xsi:type="DockLayoutGroup" Orientation="Vertical">
    <Elements>
      <Element xsi:type="DockLayoutPanelGroup" MinHeight="36" MaxHeight="36">
        <Panels>
          <DockLayoutPanel Name="ToolPanel"/>
        </Panels>
      </Element>
      <Element xsi:type="DockLayoutGroup" Orientation="Horizontal">
        <Elements>
          <Element xsi:type="DockLayoutPanelGroup" MinWidth="48" MaxWidth="64" Width="64">
            <Panels>
              <DockLayoutPanel Name="ToolsPanel"/>
            </Panels>
          </Element>
          <Element xsi:type="DockLayoutGroup" Orientation="Vertical" Width="55*">
            <Elements>
              <Element xsi:type="DockLayoutDocumentGroup" Height="75*"/>
              <Element xsi:type="DockLayoutPanelGroup" Height="25*">
                <Panels>
                  <DockLayoutPanel IsSelected="true" Name="LogPanel"/>
                </Panels>
              </Element>
            </Elements>
          </Element>
          <Element xsi:type="DockLayoutGroup" Orientation="Horizontal" Width="45*">
            <Elements>
              <Element xsi:type="DockLayoutGroup" Orientation="Vertical" Width="5*">
                <Elements>
                  <Element xsi:type="DockLayoutPanelGroup" Height="3*">
                    <Panels>
                      <DockLayoutPanel IsSelected="true" Name="ColorPanel"/>
                    </Panels>
                  </Element>
                  <Element xsi:type="DockLayoutPanelGroup" Height="7*">
                    <Panels>
                      <DockLayoutPanel IsSelected="true" Name="PropertiesPanel"/>
                    </Panels>
                  </Element>
                </Elements>
              </Element>
              <Element xsi:type="DockLayoutPanelGroup" Width="5*">
                <Panels>
                  <DockLayoutPanel IsSelected="true" Name="LayersPanel"/>
                </Panels>
              </Element>
            </Elements>
          </Element>
        </Elements>
      </Element>
    </Elements>
  </Root>
</Layout>